<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @liqours.order("venue_id ASC").each do |liqour| %>
      <tr>
        <td><%= liqour.name %></td>
        <td><%= Venue.where(id: liqour.venue_id).first.name%></td>

        <td><%= link_to 'Show', liqour %></td>
        <td><%= link_to 'Edit', edit_liqour_path(liqour) %></td>
        <td><%= link_to 'Destroy', liqour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>