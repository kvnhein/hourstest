        <!--<li class="tab bloomfield_tab"><a class="" href="#north_side">North Side</a></li>-->
      </ul>
    </div>
  </div>

<script>
   $(document).ready(function(){
    $('ul.tabs').tabs();
  });
</script>

<div id="Market_Square">
     <p class="center-tit"><%= link_to 'See Downtown Hours', downtown_path, class:"btn-flat disabled teal-text text-accent-4", 'data-no-turbolink' => true %></p>
  <%@venue_array.each do |venue| %>
     <%if venue.neighborhood_id == 5%>
         <%@venue = venue %>
         <%= render 'venue_index'%>
     <%end%>
<% end %>
</div>

