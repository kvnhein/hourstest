$('#up<%=@event.id%>').bind('ajax:success', function() {
   $('#event<%=@event.id%>').html('<%=escape_javascript (@event.cached_votes_total + @event.credits).to_s %>');
    $('#like-<%=@event.id%>').html('<%=escape_javascript (render partial: "shared/event_downvote") %>');
     $('.user-points>').html('<%=escape_javascript link_to "Points: #{current_user.experience}" %>');
<%if current_user.experience < 5 %>
Materialize.toast('Saved To Profile (+5 Hours)', 4000)
<%elsif current_user.experience < 15%>
Materialize.toast('Saved', 4000)
<%elsif current_user.experience == 50%>
Materialize.toast('Congratulations You Can Now Verify Hours!', 4000)
<%end%>
});