<script>
   $(document).ready(function(){
    $('ul.tabs').tabs();
  });
</script>

<div id="Market_Square">
     <p class="center-tit"><%= link_to 'See Downtown Hours', downtown_path, class:"btn-flat disabled teal-text text-accent-4", 'data-no-turbolink' => true %></p>
    <%@urbanist_batch = []%>
    <%@urbanist_venues.where(neighborhood_id: 5).each do |venue|%>
        <%a= venue.id%>
        <%events = @events.where(venue_id: a).to_a%>
        <%events.each do |event|%>
         <%@urbanist_batch.push(event)%>
    <%end%>
    <%end%>
     
     <%= render 'urbanist_batch'%>
</div>

