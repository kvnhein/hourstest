
<div class="like-btn">
<div class="like-like">
   <div id = 'like-<%=@j.id%>'>
      <% if @signed_in == true %>
      <% if @user_likes.include?(@j) %>
				  <%= render 'daily_specials/downvote'%>
			    <% else %>
				    <%= render 'daily_specials/upvote' %>
			    <% end %>
          <%else%>
             <%=link_to new_user_session_path  do%>
               <i class="zmdi zmdi-favorite-outline"></i>
            <%end%>
          <%end%>
     </div>
</div>

<div class="like-number" id='special<%=@j.id%>'>
   <%= @j.credit + @j.cached_votes_total%>
</div>
</div>


